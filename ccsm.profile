[unityshell]
s0_execute_command = <Super>r
s0_icon_size = 32
s0_devices_option = 2
s0_show_desktop_icon = true

[gnomecompat]
s0_main_menu_key = Disabled
s0_run_key = Disabled

[commands]
s0_command0 = wmctrl -a terminal
s0_command1 = wmctrl -a "Remote Terminal"
s0_command2 = wmctrl -a GVIM
s0_command3 = focuswm nautilus
s0_command4 = focuswm java
s0_command5 = focuswm firefox
s0_command6 = focuswm thunderbird
s0_command7 = focuswm chrome
s0_command8 = focuswm gedit
s0_command9 = focuswm pidgin
s0_run_command0_key = <Control><Primary><Alt>k
s0_run_command1_key = <Control><Primary><Alt>j
s0_run_command2_key = <Control><Primary><Alt>v
s0_run_command3_key = <Control><Primary><Alt>f
s0_run_command4_key = <Control><Primary><Alt>e
s0_run_command5_key = <Control><Primary><Alt>i
s0_run_command6_key = <Control><Primary><Alt>b
s0_run_command7_key = <Control><Primary><Alt>c
s0_run_command8_key = <Control><Primary><Alt>n
s0_run_command9_key = <Control><Primary><Alt>p

[opengl]
s0_sync_to_vblank = false

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;decor;imgpng;commands;snap;regex;mousepoll;animation;gnomecompat;place;vpswitch;grid;move;resize;session;workarounds;fade;cube;expo;ezoom;rotate;scale;unityshell;unitymtgrabhandles;
s0_hsize = 4
s0_vsize = 1

